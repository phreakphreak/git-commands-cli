{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    // "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "resolveJsonModule": true,

    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true
  }
